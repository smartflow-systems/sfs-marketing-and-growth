{
  "name": "sfs-marketing-and-growth",
  "version": "0.1.0",
  "description": "Smartflow Store MVP API with Multi-Agent Orchestration",
  "main": "server.js",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "orchestrator": "node server/orchestrator/index.js",
    "agent": "node scripts/sfs-agent-cli.js",
    "sync": "bash scripts/auto-sync.sh",
    "sync:watch": "node scripts/sync-chatgpt-claude.js watch",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "preview": "vite preview",
    "test": "echo 'no tests configured' && exit 0",
    "migrate": "prisma migrate dev --name init",
    "studio": "prisma studio",
    "dev:5100": "PORT=5100 node server.js",
    "pull": "git pull --no-edit",
    "push": "git push",
    "auto-commit": "git add . && git commit -m 'chore: auto-commit [Auto-Sync]' || true",
    "sync-all": "npm run auto-commit && npm run sync",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.13.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "stripe": "^16.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "lucide-react": "^0.462.0",
    "@heroicons/react": "^2.2.0",
    "chart.js": "^4.4.7",
    "react-chartjs-2": "^5.3.0",
    "@stripe/stripe-js": "^4.0.0",
    "@stripe/react-stripe-js": "^2.8.0",
    "date-fns": "^4.1.0",
    "react-calendar": "^5.1.0",
    "react-big-calendar": "^1.15.0",
    "framer-motion": "^11.15.0",
    "qrcode": "^1.5.4",
    "papaparse": "^5.4.1",
    "html2canvas": "^1.4.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "prisma": "^5.22.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "vite": "^6.0.5",
    "@vitejs/plugin-react": "^4.3.4",
    "terser": "^5.37.0",
    "typescript": "^5.7.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/node": "^22.10.5",
    "@types/papaparse": "^5.3.15",
    "@types/qrcode": "^1.5.5",
    "tailwindcss": "^3.4.17",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "tailwindcss-animate": "^1.0.7",
    "eslint": "^9.17.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "vitest": "^2.1.8",
    "jsdom": "^25.0.1",
    "@testing-library/react": "^16.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@vitest/ui": "^2.1.8"
  },
  "engines": {
    "node": ">=18"
  }
